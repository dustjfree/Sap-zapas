<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

  <style name="blue_edit_text" parent="@android:style/Widget.EditText">  
       <item name="android:background">@drawable/edittext_selector</item>  
   </style>  
 <style name="reg_button_new" parent="@android:style/Widget.Button">
       <item name="android:background">@drawable/button_rega_new</item>
   </style>  
   <style name="Actionbar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
       <item name="android:background">@drawable/gradient_action_bar</item>
   </style>
   <style name="windowBack" parent="@android:style/Theme.Holo.Light">
       <item name="android:windowBackground">@drawable/gradient_action_bar</item>
   </style>
    <style name="rLayoutFromItemHist" parent="@android:style/Theme.Holo.Light">
       <item name="android:background">@drawable/linear_layout1</item>
   </style>
   <style name="comments" parent="@android:style/Theme.Holo.Light">  
       <item name="android:background">@drawable/comments_48dp</item>
   </style>
   <style name="plus" parent="@android:style/Theme.Holo.Light">  
       <item name="android:background">@drawable/like</item>  
   </style>

</resources>
